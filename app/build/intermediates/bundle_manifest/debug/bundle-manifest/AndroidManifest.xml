<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beststudy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.beststudy.RadioButtonActivity" />
        <activity android:name="com.example.beststudy.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.beststudy.GradeCalc"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.beststudy.AssignmentScreen"
            android:label="BestStudy" />
        <activity
            android:name="com.example.beststudy.CalenderReminders"
            android:label="BestStudy" />
        <activity
            android:name="com.example.beststudy.WeekSchedule"
            android:label="BestStudy" />

        <receiver android:name="com.example.beststudy.NotificationReceiver" />

        <activity
            android:name="com.example.beststudy.Courseclick"
            android:label="BestStudy" />
        <activity
            android:name="com.example.beststudy.Course"
            android:label="BestStudy" />
        <activity
            android:name="com.example.beststudy.CalculateGpa"
            android:label="BestStudy" />
    </application>

</manifest>